<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
          fx:controller="controllers.interpolacion.ResultadoController" hgap="10" vgap="10">

    <padding>
        <Insets topRightBottomLeft="25"/>
    </padding>

    <Label GridPane.rowIndex="0" GridPane.columnIndex="0" fx:id="label"/>
    <TextField fx:id="tfResultado" GridPane.rowIndex="0" GridPane.columnIndex="1" minWidth="50"/>
    <HBox GridPane.rowIndex="1" GridPane.columnIndex="1" spacing="5">
        <Button text="Seguir" fx:id="btnSeguir"/>
        <Button text="Salir" fx:id="btnSalir"/>
    </HBox>

</GridPane>
